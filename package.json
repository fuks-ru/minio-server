{
  "name": "@fuks-ru/minio-server",
  "version": "1.0.0",
  "description": "Bucket for files",
  "repository": "https://github.com/DiFuks/fuks-minio-server",
  "author": "Dmitry Fuks",
  "license": "MIT",
  "packageManager": "yarn@3.2.3",
  "scripts": {
    "dev:docker": "docker run --name minio --rm -p 9000:9000 -p 9001:9001 -e \"MINIO_ROOT_USER=user123\" -e \"MINIO_ROOT_PASSWORD=secret123\" -v ${PWD}/var/data:/data minio/minio server /data --console-address \":9001\"",
    "dev:server": "cross-env NODE_ENV=development ts-node-dev -r tsconfig-paths/register --transpile-only --inspect=0.0.0.0:5005 ./src/main.ts",
    "dev": "concurrently \"yarn run dev:docker\" \"yarn run dev:server\""
  },
  "main": "./dist/lib/index.js",
  "module": "./dist/lib/index.js",
  "types": "./dist/lib/index.d.ts",
  "devDependencies": {
    "@fuks-ru/auth-client": "0.4.5",
    "@fuks-ru/auth-module": "0.0.5",
    "@fuks-ru/common": "0.0.9",
    "@fuks-ru/common-backend": "portal:../common/packages/common-backend",
    "@nestjs/common": "^9.1.2",
    "@nestjs/core": "^9.1.2",
    "@nestjs/platform-express": "^9.1.2",
    "@nestjs/swagger": "^6.1.2",
    "@rushstack/eslint-patch": "^1.2.0",
    "@types/express": "^4.17.14",
    "@types/minio": "^7.0.13",
    "@types/multer": "^1.4.7",
    "@types/uuid": "^8.3.4",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.13.2",
    "concurrently": "^7.4.0",
    "cross-env": "^7.0.3",
    "eslint": "^8.24.0",
    "eslint-config-fuks": "^1.8.3",
    "express": "^4.18.1",
    "minio": "^7.0.32",
    "nestjs-i18n": "^9.1.10",
    "nestjs-minio": "^2.2.1",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^7.5.7",
    "ts-node": "^10.9.1",
    "ts-node-dev": "^2.0.0",
    "tsconfig-paths": "^4.1.0",
    "ttypescript": "^1.5.13",
    "typescript": "4.8.3",
    "typescript-transform-paths": "^3.3.1",
    "uuid": "^9.0.0"
  }
}
